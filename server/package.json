{
  "name": "learning-tracker-server",
  "version": "1.0.0",
  "description": "Learning Tracker API server.",
  "main": "index.js",
  "scripts": {
    "test": "concurrently \"mocha --timeout 2000\" \"eslint .\"",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "restart": "concurrently \"node index.js\" \"npm run start --prefix client\"",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"nodemon --exec 'mocha'\"",
    "lint": "eslint ."
  },
  "author": "R. da Silva",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.20.1",
    "concurrently": "^4.0.1",
    "cors": "^2.8.4",
    "express": "^4.18.2",
    "mongoose": "^5.2.8",
    "react-router-dom": "^4.3.1",
    "react-select": "^3.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0"
  }
}
